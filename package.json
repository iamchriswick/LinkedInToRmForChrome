{
  "name": "LinkedInToRm",
  "short_name": "LinkedIn to RM",
  "title": "LinkedIn to RM",
  "version": "0.0.5",
  "version_name": "v.0.0.5.alpha.1",
  "description": "Copy details of candidates from LinkedIn to Recruitment Manager",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf app/build && node ./npm_scripts/build.js",
    "dev": "rm -rf app/build && node ./npm_scripts/dev.js",
    "prod": "bash ./npm_scripts/prod.sh ./app/src/key.pem",
    "chrome": "bash ./npm_scripts/chrome/chrome.sh",
    "chrome:load": "bash ./npm_scripts/chrome/load-extension.sh ./app/build",
    "install:chrome": "bash ./npm_scripts/install/google-chrome.sh",
    "install:xvfb": "bash ./npm_scripts/install/xvfb.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamchriswick/LinkedInToRmForChrome.git"
  },
